<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\AdobeStockAssetApi\Api\Data\ConfigInterface" type="Magento\AdobeStockAsset\Model\Config"/>
    <preference for="Magento\AdobeStockAssetApi\Api\ClientInterface" type="Magento\AdobeStockAsset\Model\Client"/>
    <preference for="Magento\AdobeStockAssetApi\Api\SearchRequestBuilderInterface" type="Magento\AdobeStockAsset\Model\Search\RequestBuilder"/>
    <preference for="Magento\AdobeStockAsset\Model\Request\ConfigInterface" type="Magento\AdobeStockAsset\Model\Request\Config"/>
    <preference for="Magento\AdobeStockAssetApi\Api\Data\SearchRequestInterface" type="Magento\AdobeStockAsset\Model\Search\Request"/>
    <preference for="Magento\AdobeStockAssetApi\Api\Data\AssetInterface" type="Magento\AdobeStockAsset\Model\Asset"/>
    <virtualType name="Magento\AdobeStockAsset\Model\Request\Config\SchemaLocator" type="Magento\Framework\Config\GenericSchemaLocator">
        <arguments>
            <argument name="moduleName" xsi:type="string">Magento_AdobeStockAsset</argument>
            <argument name="schema" xsi:type="string">adobe_stock_requests.xsd</argument>
        </arguments>
    </virtualType>
    <virtualType name="Magento\AdobeStockAsset\Model\Request\Config\Reader" type="Magento\Framework\Config\Reader\Filesystem">
        <arguments>
            <argument name="converter" xsi:type="object">Magento\AdobeStockAsset\Model\Request\Config\Converter</argument>
            <argument name="schemaLocator" xsi:type="object">Magento\AdobeStockAsset\Model\Request\Config\SchemaLocator</argument>
            <argument name="fileName" xsi:type="string">adobe_stock_requests.xml</argument>
        </arguments>
    </virtualType>
    <virtualType name="Magento\AdobeStockAsset\Model\Request\Config\Data" type="Magento\Framework\Config\Data">
        <arguments>
            <argument name="reader" xsi:type="object">Magento\AdobeStockAsset\Model\Request\Config\Reader</argument>
            <argument name="cacheId" xsi:type="string">adobe_stock_request_config</argument>
        </arguments>
    </virtualType>
    <type name="Magento\AdobeStockAsset\Model\Request\Config">
        <arguments>
            <argument name="dataStorage" xsi:type="object">Magento\AdobeStockAsset\Model\Request\Config\Data</argument>
        </arguments>
    </type>
</config>

